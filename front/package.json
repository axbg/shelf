{
  "name": "front",
  "version": "1.0.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && node prepare-manifest.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.19.1",
    "register-service-worker": "^1.7.2",
    "vue": "^2.6.14",
    "vue-google-signin-button": "^1.0.4",
    "vue-infinite-loading": "^2.4.5",
    "vue-material": "^1.0.0-beta-15",
    "vue-particles": "^1.0.9",
    "vue-spinner": "^1.0.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.0-beta.7",
    "@vue/cli-plugin-pwa": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@babel/eslint-parser": "^7.16.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^8.0.3",
    "prettier": "^2.4.1",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "vue": {
    "pwa": {
      "name": "shelf",
      "themeColor": "#D23669",
      "manifestOptions": {
        "background_color": "#FFF7F8",
        "icons": [
          {
            "src": "img/icons/android/android-launchericon-512-512.png",
            "sizes": "512x512",
            "type": "image/png"
          },
          {
            "src": "img/icons/android/android-launchericon-192-192.png",
            "sizes": "192x192",
            "type": "image/png"
          },
          {
            "src": "img/icons/android/android-launchericon-144-144.png",
            "sizes": "144x144",
            "type": "image/png"
          },
          {
            "src": "img/icons/android/android-launchericon-96-96.png",
            "sizes": "96x96",
            "type": "image/png"
          },
          {
            "src": "img/icons/android/android-launchericon-72-72.png",
            "sizes": "72x72",
            "type": "image/png"
          },
          {
            "src": "img/icons/android/android-launchericon-48-48.png",
            "sizes": "48x48",
            "type": "image/png"
          },
          {
            "src": "img/icons/chrome/chrome-extensionmanagementpage-48-48.png",
            "sizes": "48x48",
            "type": "image/png"
          },
          {
            "src": "img/icons/chrome/chrome-favicon-16-16.png",
            "sizes": "16x16",
            "type": "image/png"
          },
          {
            "src": "img/icons/chrome/chrome-installprocess-128-128.png",
            "sizes": "128x128",
            "type": "image/png"
          },
          {
            "src": "img/icons/firefox/firefox-marketplace-512-512.png",
            "sizes": "512x512",
            "type": "image/png"
          },
          {
            "src": "img/icons/firefox/firefox-marketplace-128-128.png",
            "sizes": "128x128",
            "type": "image/png"
          },
          {
            "src": "img/icons/firefox/firefox-general-256-256.png",
            "sizes": "256x256",
            "type": "image/png"
          },
          {
            "src": "img/icons/firefox/firefox-general-128-128.png",
            "sizes": "128x128",
            "type": "image/png"
          },
          {
            "src": "img/icons/firefox/firefox-general-90-90.png",
            "sizes": "90x90",
            "type": "image/png"
          },
          {
            "src": "img/icons/firefox/firefox-general-64-64.png",
            "sizes": "64x64",
            "type": "image/png"
          },
          {
            "src": "img/icons/firefox/firefox-general-48-48.png",
            "sizes": "48x48",
            "type": "image/png"
          },
          {
            "src": "img/icons/firefox/firefox-general-32-32.png",
            "sizes": "32x32",
            "type": "image/png"
          },
          {
            "src": "img/icons/firefox/firefox-general-16-16.png",
            "sizes": "16x16",
            "type": "image/png"
          }
        ]
      },
      "workboxPluginMode": "InjectManifest",
      "workboxOptions": {
        "swSrc": "src/service-worker.js"
      },
      "iconPaths": {
        "appleTouchIcon": null,
        "maskIcon": null,
        "msTileImage": null
      }
    },
    "productionSourceMap": false
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
